    "~TODO": {
      "background": "#FFD703",
      "iconColour": "#FFD703",
      "icon": "checklist"
    },
    "~FIXME": {
      "background": "#FF01FF",
      "iconColour": "#FF01FF",
      "icon": "flame"
    },
    "~HACK": {
      "background": "#157EFB",
      "iconColour": "#157EFB",
      "icon": "rocket"
    },
    "~BUG": {
      "background": "#fb2c15",
      "iconColour": "#fb2c15",
      "icon": "bug"
    },
    "~MARK": {
      "background": "#fb8c15",
      "iconColour": "#fb8c15",
      "icon": "tag"
    },
    "[ ]": {
      "background": "#fc2e46",
      "iconColour": "#fc2e46",
      "icon": "square"
